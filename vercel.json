{
    "builds": [
      {
        "src": "VCube_Data_API/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "VCube_Data_API/wsgi.py"
      }
    ]
  }
  
