{
    "builds": [{
      "src": "VCube_Data_API/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.5" }
    },{
        "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
          "distDir" : "staticfiles_build"
      }
    }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        },
      {
        "src": "/(.*)",
        "dest": "VCube_Data_API/wsgi.py"
      }
    ]
}
